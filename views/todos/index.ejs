<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>All Todos</title>
  </head>
  <body>
    <h1>All Todos</h1>

    <% todos.forEach(todo => { %>
      <div class="todo-item">
        <h3><%= todo.title %></h3>
        <p><%= todo.description %></p>
      </div>
    <% }) %>
  </body>
</html>
